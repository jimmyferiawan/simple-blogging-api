{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "paths": {
      "@routes/*": ["./routes/*"],
      "@middlewares/*": ["./middlewares/*"],
      "@services/*": ["./services/*"],
      "@models/*": ["./models/*"],
      "@helpers/*": ["./helpers/*"],
      "@configs/*": ["./configs/*"]
    },
    "baseUrl": "."
  },
  "module": {
    "type": "commonjs"
  }
}
